{
  "name": "chart_ssr",
  "description": "A chart_ssr project",
  "author": "SampsonLi <740056710@qq.com>",
  "private": true,
  "scripts": {
    "dev": "node server-dev",
    "test": "node server-not-ssr-dev",
    "start": "cross-env NODE_ENV=production node server --harmony",
    "build": "npm run build:client && npm run build:server",
    "build:client": "cross-env NODE_ENV=production webpack --config build/webpack.client.config.js --progress --hide-modules",
    "build:server": "cross-env NODE_ENV=production webpack --config build/webpack.server.config.js --progress --hide-modules",
    "fstart": "cross-env NODE_ENV=production forever start -a -l htmlssr.log server.js",
    "frestart": "cross-env NODE_ENV=production forever restart server.js",
    "fstop": "forever stop server.js",
    "buildAndRestart": "npm run build && npm run frestart",
    "viewlogs": "tail -f -n 50 /data/waplogs/html_ssr/datachart.log",
    "lint": "eslint --ext .js,.vue src",
    "lint-fix": "eslint --ext .js,.vue src --fix",
    "precommit": "npm run lint"
  },
  "dependencies": {
    "axios": "^0.15.0",
    "es6-promise": "^4.0.5",
    "express": "^4.14.0",
    "fastclick": "^1.0.6",
    "fetch-jsonp": "^1.0.6",
    "husky": "^0.13.2",
    "lru-cache": "^4.0.1",
    "scroller": "0.0.3",
    "serve-favicon": "^2.3.0",
    "v-tap": "^2.1.2",
    "vue": "^2.1.10",
    "vue-router": "^2.2.0",
    "vue-server-renderer": "^2.1.10",
    "vuex": "^2.1.1",
    "vuex-router-sync": "^4.1.2"
  },
  "devDependencies": {
    "autoprefixer": "^6.7.2",
    "babel-core": "^6.22.1",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.10",
    "babel-plugin-transform-object-rest-spread": "^6.22.0",
    "babel-preset-es2015": "^6.22.0",
    "co": "^4.6.0",
    "cross-env": "^3.1.4",
    "css-loader": "^0.26.1",
    "ejs": "^2.5.5",
    "eslint": "^3.17.1",
    "eslint-config-standard": "^7.0.1",
    "eslint-friendly-formatter": "^2.0.7",
    "eslint-loader": "^1.6.3",
    "eslint-plugin-html": "^2.0.1",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^2.1.1",
    "eslint-plugin-vue": "^2.0.1",
    "file-loader": "^0.10.0",
    "forever": "^0.15.2",
    "forward-request": "0.0.2",
    "html-webpack-plugin": "^2.28.0",
    "log4js": "^1.1.0",
    "stylus": "^0.54.5",
    "stylus-loader": "^2.1.2",
    "url-loader": "^0.5.7",
    "vue-loader": "^10.3.0",
    "vue-template-compiler": "^2.1.10",
    "webpack": "^2.1.1",
    "webpack-dev-middleware": "^1.10.0",
    "webpack-hot-middleware": "^2.16.1"
  }
}
